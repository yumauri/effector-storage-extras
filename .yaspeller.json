{
  "excludeFiles": [
    ".git",
    ".idea",
    ".vscode",
    "node_modules",
    "build",
    "coverage",
    ".nyc_output"
  ],
  "lang": "en",
  "fileExtensions": [".md"],
  "dictionary": [
    "async",
    "asynchronously",
    "AsyncStorage",
    "clonable",
    "debounce",
    "EncryptedStorage",
    "idb",
    "IndexedDB",
    "io-ts",
    "isomorphic",
    "keyArea",
    "keyval",
    "localStorage",
    "noop",
    "patronum",
    "Payload",
    "pushState",
    "replaceState",
    "rn",
    "runtypes",
    "sessionStorage",
    "Setplex",
    "StorageAdapter",
    "superstruct",
    "timestamp",
    "unhandled",
    "unsubscribe",
    "UseStore",
    "zod"
  ],
  "ignoreText": [],
  "ignoreUrls": true,
  "ignoreUppercase": true,
  "findRepeatWords": false,
  "maxRequests": 5
}
